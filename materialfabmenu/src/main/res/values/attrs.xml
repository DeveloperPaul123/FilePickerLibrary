<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FloatingActionButton">
        <attr name="fab_colorPressed" format="color"/>
        <attr name="fab_colorDisabled" format="color"/>
        <attr name="fab_colorNormal" format="color"/>
        <attr name="fab_icon" format="reference"/>
        <attr name="fab_size" format="enum">
            <enum name="normal" value="0"/>
            <enum name="mini" value="1"/>
        </attr>
        <attr name="fab_title" format="string"/>
        <attr name="fab_stroke_visible" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="AddFloatingActionButton">
        <attr name="fab_plusIconColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="MaterialFABMenuItem">
        <attr name="mat_fab_item_icon" format="reference"/>
    </declare-styleable>

    <declare-styleable name="MaterialFloatingActionButtonMenu">
        <attr name="mat_fab_menu_color" format="color"/>
        <attr name="mat_fab_menu_showChildrenAlways" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="MaterialFloatingActionButton">
        <attr name="mat_fab_colorNormal" format="color"/>
        <attr name="mat_fab_colorPressed" format="color"/>
        <attr name="mat_fab_use_selector" format="boolean"/>
        <attr name="mat_fab_icon" format="reference"/>
        <attr name="mat_fab_title" format="string"/>
        <attr name="mat_fab_size" format="enum">
            <enum name ="normal" value="0"/>
            <enum name="mini" value="1"/>
        </attr>
    </declare-styleable>

</resources>